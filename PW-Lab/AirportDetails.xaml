<Window x:Class="PW_Lab.AirportDetails"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PW_Lab"
        mc:Ignorable="d"
        Title="AirportDetails" Height="450" Width="800">
    <StackPanel>
        <Label Content="{Binding ICAOLabel}"></Label>
        <Label Content="{Binding IATALabel}"></Label>
        <Label Content="{Binding PassengerCountLabel}"></Label>
        <Label Content="{Binding VoivodeshipLabel}"></Label>
        <Label Content="{Binding CityLabel}"></Label>
    </StackPanel>
</Window>
